            color: #000;
            text-decoration: none;
        }

        nav {
            display: flex;
            justify-content: space-between;
            align-items: center;
            margin-bottom: 5px;
            font-size: large;
            padding: 20px;
        }

        nav ul {
            list-style: none;
            margin: 0;
            padding: 0;
            display: flex;
        }

        nav ul li {
            margin-right: 20px;
        }

        nav ul li a {
            text-decoration: none;
            color: #000;
            font-weight: bold;
        }
        body {
            font-family: Arial, sans-serif;
            background-size: cover;
            background-position: center;
            background-repeat: no-repeat;
            background-color: rgb(255, 255, 255);
        }

        .lista-productos {
            list-style: none;
            margin: 0;
            padding: 0;
            display: flex;
            flex-wrap: wrap;
            justify-content: center;
        }

        .lista-productos li {
            background-color: #fff;
            box-shadow: 0px 0px 5px 0px rgba(0, 0, 0, 0.3);
            margin: 20px;
            width: 300px;
            text-align: center;
            position: relative;
            overflow: hidden;
            border-radius: 10px;
            cursor: pointer;
        }

        .lista-productos li img {
            width: 100%;
            height: 200px;
            object-fit: cover;
            border-radius: 10px 10px 0 0;
            transition: transform 0.2s ease-in-out;
        }

        .lista-productos li img:hover {
            transform: scale(1.1);
        }

        .lista-productos li.pressed img {
            transform: scale(1.5);
            z-index: 1;
        }

        .lista-productos li p {
            font-size: 16px;
            line-height: 1.5;
            margin: 10px 0;
        }

        .lista-productos li .precio {
            font-size: 18px;
            font-weight: bold;
            color: #c00;
        }

        .lista-productos li .boton {
            /*
            position: fixed;
            */
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            margin-right: 20px;
            transform: scale(1.2);
        }
    </style>

</head>

